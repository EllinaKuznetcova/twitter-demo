<% if current_user %>
  <div>
    <h2>Dashboard</h2>
    <p>
      Welcome, <%= current_user.name %>
    </p>
    <% tweets.each do |tweet| %>
      Text: <%= tweet[:full_text] %><br />
      Author: <%= tweet[:name] %><br /><br />
    <% end %>
  </div>
<% else %>
  <div>

    <p>
      <%= link_to "Sign in with Twitter", "/auth/twitter" %>
    </p>
  </div>
<% end %>